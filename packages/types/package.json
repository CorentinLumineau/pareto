{
  "name": "@pareto/types",
  "version": "0.0.0",
  "private": true,
  "description": "Shared TypeScript types for Pareto Comparator",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@pareto/typescript-config": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  }
}
